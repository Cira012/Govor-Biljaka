{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "plant-observations"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "plants-db",
      "collectionName": "plant-observations",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
